    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "def f(x):\n",
    "    return 1/x\n",
    "fx_name = r'$f(x)=\\frac{1}{x}$'\n",
    "x=np.linspace(0.01,2.01,101)\n",
    "y=f(x)\n",
    "plt.plot(x, y, label=fx_name)\n",
    "plt.legend(loc='upper left')\n",
    "plt.xlabel('x-axis')\n",
    "plt.ylabel('y-axis')\n",
    "plt.title('This is the function 1/x')\n",
    "plt.show()\n",
